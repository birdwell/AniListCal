{
    "entry": [
        "server/index.ts",
        "client/src/main.tsx",
        "client/src/pages/**/*.tsx",
        "client/src/components/**/*.tsx"
    ],
    "extensions": [
        ".ts",
        ".tsx",
        ".js",
        ".jsx"
    ],
    "ignorePatterns": [
        "**/node_modules/**",
        "**/dist/**",
        "**/coverage/**",
        "**/*.test.ts",
        "**/*.test.tsx",
        "**/*.spec.ts",
        "**/*.spec.tsx",
        "**/generated/**"
    ]
}